<h1>プロフィール編集</h1>

<%= form_with(model: @user_profile, url: users_update_path, local: true, method: :post, html: { class: 'recipe-form' } ) do |f| %>



  <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
      <%= f.label :country %>
      <%= f.text_field :country_id, class: 'form-control' %>
  </div>

  <%= f.submit "保存", class: 'btn btn-primary' %>
<% end %>